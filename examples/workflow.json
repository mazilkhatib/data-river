{
  "id": "example-workflow",
  "blocks": [
    {
      "id": "start-block",
      "type": "start",
      "inputs": {},
      "outputs": {},
      "onError": null
    },
    {
      "id": "input-block",
      "type": "input",
      "inputs": {},
      "outputs": {}
    },
    {
      "id": "logic-block",
      "type": "logic",
      "inputs": { "input": "{{input-block.userInput}}" },
      "outputs": {},
      "condition": "input === 'Yes'"
    },
    {
      "id": "output-block",
      "type": "output",
      "inputs": { "result": "{{logic-block.conditionResult}}" },
      "outputs": {}
    },
    {
      "id": "end-block",
      "type": "end",
      "inputs": {},
      "outputs": {}
    }
  ],
  "connections": [
    { "from": "start-block", "to": "input-block" },
    { "from": "input-block", "to": "logic-block" },
    { "from": "logic-block", "to": "output-block" },
    { "from": "output-block", "to": "end-block" }
  ],
  "environment": {
    "variables": {}
  },
  "config": {
    "maxConcurrentTasks": 5,
    "supportsWebSocket": true,
    "executionContext": "browser",
    "retryOnFailure": true
  }
}
